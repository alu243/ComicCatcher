{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "vue@3.3.4",
      "destination": "wwwroot/vue/vue/",
      "files": [
        "vue.global.js",
        "vue.global.min.js",
        "vue.global.prod.js",
        "vue.global.prod.min.js"
      ]
    },
    {
      "library": "bootstrap@5.3.1",
      "destination": "wwwroot/bootstrap/",
      "files": [
        "js/bootstrap.bundle.js",
        "css/bootstrap.min.css",
        "js/bootstrap.bundle.min.js"
      ]
    },
    {
      "library": "bootstrap-icons@1.10.5",
      "destination": "wwwroot/bootstrap-icons/",
      "files": [
        "bootstrap-icons.svg",
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.css",
        "font/bootstrap-icons.json",
        "font/bootstrap-icons.min.css"
      ]
    }
  ]
}